    "@backstage/core-plugin-api": "^1.7.0",
    "@backstage/plugin-catalog-react": "^1.8.5",
    "@backstage/theme": "^0.4.3",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@mui/x-charts": "^6.18.7",
